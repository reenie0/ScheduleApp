@model IEnumerable<ScheduleApp.Models.Student>

@{
    ViewData["Title"] = "Students";
}

<h1>Students</h1>

<table class="table">
    <thead>
        <tr>
            <th>StudentId</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>StudentNumber</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
    {
        <tr>
                <td>@student.StudentId</td>
                <td>@student.FirstName</td>
                <td> @student.LastName</td>
                <td> @student.StudentNumber</td>
                <td>@student.Email</td>
            </tr>
    }
    </tbody>
</table>
